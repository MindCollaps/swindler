{
  "name": "swindler",
  "private": false,
  "type": "module",
  "version": "0.0.3-alpha",
  "packageManager": "bun@1.3.5",
  "scripts": {
    "build": "nuxt build",
    "dev": "docker compose -f docker-compose.yml -f docker-compose.dev.yml up --build",
    "dev:down": "docker compose -f docker-compose.yml -f docker-compose.dev.yml down",
    "dev:local": "nuxt dev",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "prepare": "nuxt prepare",
    "postinstall": "nuxt prepare",
    "lint:ts": "eslint .",
    "lint:ts:fix": "eslint . --fix",
    "stylelint": "stylelint --max-warnings=0 './app/**/*.{vue,scss,css}'",
    "stylelint:fix": "stylelint --fix './app/**/*.{vue,scss,css}'",
    "lint:fix": "bun run stylelint:fix && bun run lint:ts:fix",
    "lint": "bun run stylelint && bun run lint:ts",
    "db-push": "docker compose -f docker-compose.yml -f docker-compose.dev.yml exec swindler-app bun run prisma db push",
    "db-deploy": "docker compose -f docker-compose.yml -f docker-compose.dev.yml exec swindler-app bun run prisma migrate deploy",
    "db-drop": "docker compose -f docker-compose.yml -f docker-compose.dev.yml exec swindler-app bun run prisma db push --force-reset",
    "db-reset-db": "docker compose -f docker-compose.yml -f docker-compose.dev.yml exec swindler-app sh -c \"bun run prisma migrate reset --force && bun run prisma migrate dev --name init && bun run prisma generate\"",
    "db-reset": "rm -rf ./prisma/migrations && bun run db-reset-db",
    "sort": "python .config/sort_wordlists.py",
    "atlas": "python .config/generate_avatar_atlas.py"
  },
  "dependencies": {
    "@nuxt/fonts": "^0.12.1",
    "@nuxt/icon": "^2.1.1",
    "@nuxt/image": "^2.0.0",
    "@nuxt/scripts": "^0.13.2",
    "@pinia/nuxt": "^0.11.3",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@socket.io/redis-adapter": "^8.3.0",
    "@unhead/vue": "^2.1.1",
    "@vueuse/core": "^14.1.0",
    "@vueuse/motion": "^3.0.3",
    "animejs": "^4.2.2",
    "bcrypt": "^6.0.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "engine.io": "^6.6.5",
    "ioredis": "^5.8.2",
    "jsonwebtoken": "^9.0.3",
    "nuxt": "^4.2.2",
    "nuxt-lottie": "^1.0.8",
    "pg": "^8.16.3",
    "pinia": "^3.0.4",
    "prisma": "^7.2.0",
    "redlock-universal": "^0.7.7",
    "socket.io": "^4.8.3",
    "socket.io-client": "^4.8.3",
    "uuid": "^13.0.0",
    "vite-svg-loader": "^5.1.0",
    "vite-tsconfig-paths": "^6.0.3",
    "vue": "^3.5.26",
    "vue-router": "^4.6.4",
    "zod": "^4.3.2"
  },
  "devDependencies": {
    "@eslint/js": "^9.30.0",
    "@iconify-json/material-symbols": "^1.2.28",
    "@nuxt/devtools": "^2.6.0",
    "@nuxt/eslint": "^1.4.1",
    "@nuxtjs/stylelint-module": "^5.2.1",
    "@stylistic/eslint-plugin": "^5.1.0",
    "@stylistic/stylelint-plugin": "^4.0.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie": "^1.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/redis": "^4.0.11",
    "eslint": "^9.30.0",
    "eslint-plugin-vue": "^10.2.0",
    "postcss": "^8.5.6",
    "postcss-html": "^1.8.0",
    "prettier": "^3.6.2",
    "sass-embedded": "^1.89.2",
    "stylelint": "^16.21.0",
    "stylelint-config-clean-order": "^7.0.0",
    "stylelint-config-recommended-scss": "^16.0.2",
    "stylelint-config-recommended-vue": "^1.6.1",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.1",
    "vite-plugin-eslint2": "^5.0.3",
    "vue-tsc": "^3.1.5"
  }
}
